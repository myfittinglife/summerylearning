<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">
<ScrollView
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <Button
            android:id="@+id/btn_eventbus_activity"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="EventBus"
            android:textAllCaps="false"/>
        <Button
            android:id="@+id/btn_start_for_result"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="StartActivityForResult"
            android:textAllCaps="false"/>
        <Button
            android:id="@+id/btn_okgo_activity"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="OkGo网络框架"
            android:textAllCaps="false"/>

        <Button
            android:id="@+id/btn_matisse_activity"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Matisse图片选择器"
            android:textAllCaps="false"/>


        <Button
            android:id="@+id/btn_lable_activity"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="兴趣标签" />
        <Button
            android:id="@+id/btn_callback"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="接口回调" />
        <Button
            android:id="@+id/btn_expand_list"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="二级列表" />
        <Button
            android:id="@+id/btn_vibrate_ring"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="震动响铃工具类" />
        <Button
            android:id="@+id/btn_downloadmanager"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="DownloadManager系统下载管理器"
            android:textAllCaps="false"/>
        <Button
            android:id="@+id/btn_banner"
            android:text="轮播功能(多看封装解耦思想)"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
        <Button
            android:id="@+id/btn_refreshAndload"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="上拉加载下拉刷新" />
        <Button
            android:id="@+id/btn_clear_cache"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="清除缓存" />
        <Button
            android:id="@+id/btn_pulldown_menu"
            android:text="下拉菜单"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
